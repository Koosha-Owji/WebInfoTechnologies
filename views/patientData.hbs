    <div class="row_containers">
        <div class="patient_rows">
                {{#each data}}
                <div class="patient_info"> {{this.first_name}}, {{this.last_name}}
                {{!-- {{Check if the data has been added today, display check if true and cross if not}} --}}
                {{#if this.glucose_today}} Glucose added today {{this.glucose}}
                    {{#if (isGreater this.glucose this.glucose_upper)}} Greater than upper 
                    {{else if (isLess this.glucose this.glucose_lower)}} Less than lower
                    {{else}} Glucose at healthy level
                    {{/if}}
        
                {{else}}
                    Glucose Not added {{this.glucose}}
                    {{#if (isGreater this.glucose this.glucose_upper)}} yesterday Greater than upper 
                    {{else if (isLess this.glucose this.glucose_lower)}} yesterday Less than lower
                    {{else}} yesterday Glucose at healthy level
                    {{/if}}
                {{/if}}

                {{/each}}
                </div>
        </div>
    </div>